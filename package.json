    "e2e:open": "cypress open",
    "e2e:headless": "cypress run --headless",
    "test:e2e": "start-server-and-test start http://localhost:4300 cypress:run",
    "test:e2e:open": "start-server-and-test start http://localhost:4300 cypress:open"
